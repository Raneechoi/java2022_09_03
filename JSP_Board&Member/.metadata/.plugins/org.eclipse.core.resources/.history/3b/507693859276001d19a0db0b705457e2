	public int deleteFile(String imageFileName, String savePath) {
		boolean isDel = true;  // 삭제가 잘 이루어졌는지 체크하는 변수
		log.info(">>> deleteFile method 접근");
		log.info(">>> imageFileName : "+imageFileName);
	}
}
